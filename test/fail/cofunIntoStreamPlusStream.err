type  Unit : Set
term  unit : < unit : Unit >
type  Bool : Set
term  true : < true : Bool >
term  false : < false : Bool >
type  Twice : ++(A : Set) -> Set
term  inl : .[A : Set] -> ^(y0 : A) -> < inl y0 : Twice A >
term  inr : .[A : Set] -> ^(y0 : A) -> < inr y0 : Twice A >
term  fmap : .[A : Set] -> .[B : Set] -> (A -> B) -> Twice A -> Twice B
{ fmap [A] [B] f (inl a) = inl (f a)
; fmap [A] [B] f (inr a) = inr (f a)
}
type  BStr : - Size -> Set
term  cons : .[i : Size] -> ^(head : Bool) -> ^(tail : BStr i) -> < cons i head tail : BStr $i >
term  head : .[i : Size] -> (cons : BStr $i) -> Bool
{ head [i] (cons [.i] #head #tail) = #head
}
term  tail : .[i : Size] -> (cons : BStr $i) -> BStr i
{ tail [i] (cons [.i] #head #tail) = #tail
}
term  idAndLast : .[i : Size] -> BStr i -> Twice (BStr i)
