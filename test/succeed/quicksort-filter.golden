term  n9 : Nat
term  n9 = Nat.succ n8
term  l : List #
term  l = List.cons [#] n1 (List.cons [#] n3 (List.cons [#] n0 (List.cons [#] n2 (List.nil [#]))))
term  l' : List #
term  l' = quicksort [#] l
